<p id="notice"><%= notice %></p>

<h1>Patients</h1>

<ul>
  <% @patients.each do |patient| %>
    <li>
      <strong>Name:</strong>
      <%= patient.name %>
      <strong>Number of Appointments:</strong>
      <%= patient.appointments.size %>
    </li>
  <% end %>
</ul>

<%= link_to 'New Patient', new_patient_path %>
